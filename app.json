{
  "expo": {
    "scheme": "toydelapp",
    "plugins": [
      [
        "@react-native-google-signin/google-signin"
      ]
    ],
    "name": "ToyBox",
    "slug": "ToyDelApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#8B78FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icon.png",
      "bundleIdentifier": "com.toydelapp",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "intentFilters": [
        {
          "autoVerify": true,
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "toyboxlink.page.link/link"
            },
            {
              "scheme": "https",
              "host": "toyboxlink.page.link",
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#e3aeff"
      },
      "package": "com.toydelapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6f9e05d9-24ad-44d0-9171-1f4a7f6e1ad8"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/6f9e05d9-24ad-44d0-9171-1f4a7f6e1ad8"
    }
  }
}
